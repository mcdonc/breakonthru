# >>> from cryptacular.bcrypt import BCRYPTPasswordManager
# >>> manager = BCRYPTPasswordManager()
# >>> manager.encode('123')
# '$2a$10$yAaGZQsYgEIhs8bKMi2Pd.iKIGVTvUjW49aXXEfkTfGVCtcsnDTLO'
